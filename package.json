{
  "name": "sztunethelper",
  "version": "0.9.9-alpha",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://www.electronjs.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac-x86": "npm run build && electron-builder --mac --config",
    "build:mac-arm64": "npm run build && electron-builder --arm64 --mac --config",
    "build:linux-x86": "npm run build && electron-builder --linux --config",
    "build:linux-arm64": "npm run build && electron-builder --arm64 --linux --config",
    "prepare": "husky install",
    "test": "vitest",
    "dev:web": "vite dev --mode=development",
    "build:web": "vite build --mode=production",
    "preview:web": "vite preview"
  },
  "dependencies": {
    "@arco-design/web-react": "^2.45.0",
    "@electron-toolkit/preload": "^1.0.2",
    "@electron-toolkit/utils": "^1.0.2",
    "@monaco-editor/react": "^4.4.6",
    "@react-pdf/renderer": "^3.1.5",
    "@reduxjs/toolkit": "^1.8.6",
    "autoprefixer": "^10.4.13",
    "axios": "^1.2.3",
    "electron-store": "^8.1.0",
    "electron-updater": "^4.6.5",
    "execa": "npm:@esm2cjs/execa@6.1.1-cjs.1",
    "got": "^11.8.3",
    "iconv-lite": "^0.6.3",
    "lodash-es": "^4.17.21",
    "magic-design-react": "^0.0.20",
    "monaco-editor": "^0.36.0",
    "node-schedule": "^2.1.0",
    "nodemailer": "^6.9.0",
    "postcss": "^8.4.18",
    "process": "^0.11.10",
    "qiniu-js": "^3.4.1",
    "react-activation": "^0.12.4",
    "react-redux": "^8.0.4",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.2.0",
    "socket.io-client": "^4.6.0",
    "systeminformation": "^5.12.12",
    "tailwindcss": "^3.2.1",
    "wifi-name": "^3.1.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.2.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@testing-library/react": "^14.0.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "16.11.22",
    "@types/node-schedule": "^2.1.0",
    "@types/nodemailer": "^6.4.7",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/react-router": "^5.0.0",
    "@types/react-router-dom": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@vitejs/plugin-react": "^2.1.0",
    "ajv": "^8.11.0",
    "commitlint": "^17.2.0",
    "del-cli": "^5.0.0",
    "electron": "^20.3.2",
    "electron-builder": "^23.6.0",
    "electron-notarize": "^1.2.1",
    "electron-vite": "^1.0.11",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.55.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "vitest": "^0.24.5"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "node --max_old_space_size=8192 ./node_modules/eslint/bin/eslint.js --fix --color --cache --quiet"
    ],
    "*.{js,jsx,mjs,mjsx,cjs,cjsx}": [
      "node --max_old_space_size=8192 ./node_modules/eslint/bin/eslint.js --fix --color --cache --quiet"
    ]
  }
}
